\utils\\fiexed.js",[],["76","77"],"C:\\Users\\luodianlei\\Desktop\\react项目\\04-上课代码\\day20\\react-project0910\\src\\pages\\Home\\Home.js",[],"C:\\Users\\luodianlei\\Desktop\\react项目\\04-上课代码\\day20\\react-project0910\\src\\pages\\Login\\Login.js",["78","79","80"],"C:\\Users\\luodianlei\\Desktop\\react项目\\04-上课代码\\day20\\react-project0910\\src\\App.js",[],["81","82"],"C:\\Users\\luodianlei\\Desktop\\react项目\\04-上课代码\\day20\\react-project0910\\src\\api\\register.js",[],"C:\\Users\\luodianlei\\Desktop\\react项目\\04-上课代码\\day20\\react-project0910\\src\\pages\\Register\\RegisterCode\\index.js",["83","84"],"import React, { Component } from 'react'\r\nimport {\r\n  NavBar,\r\n  Icon,\r\n  InputItem,\r\n  WingBlank,\r\n  WhiteSpace,\r\n  Button,\r\n  Modal,\r\n  Toast\r\n} from 'antd-mobile'\r\nimport { createForm } from 'rc-form'\r\n\r\nimport { sendCode } from '../../../api/login'\r\nimport './index.css'\r\nimport msg from './msg.png'\r\nclass RegisterCode extends Component {\r\n  state = {\r\n    // 倒计时的时间\r\n    downcount: 5,\r\n    isDisabled: true\r\n  }\r\n  componentDidMount() {\r\n    // 直接给后台发送请求,让后台给用户发送验证码\r\n    // 从本地缓存中获取电话号\r\n    this.sendCodeFn()\r\n  }\r\n  //封装发送验证码的函数\r\n  sendCodeFn = async () => {\r\n    const phone = localStorage.getItem('phone')\r\n    const res = await sendCode(phone)\r\n    if (res.data.success) {\r\n      Toast.success('验证码发送成功', 1)\r\n\r\n      //倒计时\r\n      this.timeid = setInterval(() => {\r\n        if (this.state.downcount === 1) {\r\n          clearInterval(this.timeid)\r\n          this.setState({\r\n            downcount: 5\r\n          })\r\n          return\r\n        }\r\n        this.setState({\r\n          downcount: this.state.downcount - 1\r\n        })\r\n      }, 1000)\r\n    }\r\n  }\r\n\r\n  // 发送验证码的函数\r\n  sendVerifyCode = () => {\r\n    if (this.state.downcount === 5) {\r\n      //发送请求,发送验验证码\r\n      // console.log(1111111)\r\n      this.sendCodeFn()\r\n    }\r\n  }\r\n\r\n  // 校验验证码\r\n  validator = (rules, value, cb) => {\r\n    // console.log(rules)\r\n    // console.log(value)\r\n    // 判断用户输入的内容是否是手机号\r\n    if (/^\\d{6}$/.test(value)) {\r\n      //是手机号\r\n      // console.log('是手机号')\r\n      this.setState({\r\n        isDisabled: false\r\n      })\r\n    } else {\r\n      //不是手机号\r\n      this.setState({\r\n        isDisabled: true\r\n      })\r\n    }\r\n  }\r\n\r\n  // 发送请求验证验证码是否正确\r\n  handleVerifyCode = () => {}\r\n  render() {\r\n    const { getFieldProps } = this.props.form\r\n    return (\r\n      <div className='wrap'>\r\n        <NavBar\r\n          mode='light'\r\n          icon={<Icon type='left' color='#000' />}\r\n          onLeftClick={() => {}}\r\n        >\r\n          硅谷注册\r\n        </NavBar>\r\n        <WhiteSpace size='lg'></WhiteSpace>\r\n\r\n        <WingBlank size='lg'>\r\n          <div className='img'>\r\n            <img src={msg} alt='' />\r\n          </div>\r\n          <WhiteSpace size='lg'></WhiteSpace>\r\n          <WingBlank size='lg'>\r\n            <div>\r\n              我们将以短信或电话的形式将验证码发送给您，请注意接听0575/025/0592/010等开头的电话\r\n            </div>\r\n          </WingBlank>\r\n          <WhiteSpace size='lg'></WhiteSpace>\r\n\r\n          <div className='code'>\r\n            <InputItem\r\n              {...getFieldProps('code', {\r\n                rules: [\r\n                  {\r\n                    validator: this.validator\r\n                  }\r\n                ]\r\n              })}\r\n              clear\r\n              placeholder='请输入手机验证码'\r\n            ></InputItem>\r\n            <button\r\n              className={\r\n                this.state.downcount === 5 ? 'code-btn active' : 'code-btn'\r\n              }\r\n              onClick={this.sendVerifyCode}\r\n            >\r\n              {this.state.downcount === 5\r\n                ? '获取验证码'\r\n                : `重新发送(${this.state.downcount}s)`}\r\n            </button>\r\n          </div>\r\n          <WingBlank size='lg'>\r\n            <Button\r\n              className='code-next'\r\n              type='warning'\r\n              disabled={this.state.isDisabled}\r\n              onClick={this.handleVerifyCode}\r\n            >\r\n              下一步\r\n            </Button>\r\n          </WingBlank>\r\n          <WingBlank size='lg'>\r\n            遇到问题了?\r\n            <a href=''>请联系客服</a>\r\n          </WingBlank>\r\n        </WingBlank>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nexport default createForm()(RegisterCode)\r\n",{"ruleId":"85","severity":1,"message":"86","line":38,"column":15,"nodeType":"87","endLine":38,"endColumn":27},{"ruleId":"85","severity":1,"message":"86","line":39,"column":15,"nodeType":"87","endLine":47,"endColumn":16},{"ruleId":"85","severity":1,"message":"86","line":69,"column":15,"nodeType":"87","endLine":69,"endColumn":26},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"88","replacedBy":"92"},{"ruleId":"90","replacedBy":"93"},{"ruleId":"85","severity":1,"message":"86","line":38,"column":15,"nodeType":"87","endLine":38,"endColumn":27},{"ruleId":"85","severity":1,"message":"86","line":39,"column":15,"nodeType":"87","endLine":47,"endColumn":16},{"ruleId":"85","severity":1,"message":"86","line":69,"column":15,"nodeType":"87","endLine":69,"endColumn":26},{"ruleId":"88","replacedBy":"94"},{"ruleId":"90","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":9,"column":3,"nodeType":"98","messageId":"99","endLine":9,"endColumn":8},{"ruleId":"85","severity":1,"message":"86","line":143,"column":13,"nodeType":"87","endLine":143,"endColumn":24},"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","no-native-reassign",["100"],"no-negated-in-lhs",["101"],["100"],["101"],["100"],["101"],"no-unused-vars","'Modal' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]